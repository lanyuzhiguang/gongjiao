body{
    font-family: "苹方";
    background: #f1f3f7;
}
*{
    padding: 0;
    margin: 0;
    font-family: "苹方";
}
.rulesHead{
    margin-top: 10px;
    margin-left: 5px;
    overflow: hidden;
    background: #ffffff;
    width:98%;
    padding-bottom: 70px;
}
.rulesHead>ul{
    background: #f1f3f7;
    overflow: hidden;
}
.rulesHead>ul>li{
    list-style: none;
    width: 200px;
    height: 44px;
    border-radius: 6px 6px 0 0;
    background: #ffffff;
    float: left;
    text-align: center;
    line-height: 44px;
    margin-right: 4px;
    font-size: 16px;
    color:#797e80;
    cursor: pointer;
}
.topInput{
    margin-top: 17px;
    margin-left: 89px;
}
.topInput div{
    display: inline-block;
    font-size: 14px;
}
.topInput div:nth-child(1),.topInput div:nth-child(3){
    color:#797e80;
}
.topInput>div:first-child{
    margin-right: 18px;

}
.topInput>div:nth-child(2) input{
    width: 214px;
    height: 30px;
    border: 1px solid #d2d2d2;
    border-radius: 2px;
    outline: none;
}
.topInput>div:nth-child(3){
    margin-left: 10px;
    margin-right: 18px;
}
.topInput>div:nth-child(4){
    width: 127px;
    height: 27px;
    position: relative;
}
.topInput>div:nth-child(4)>input{
    width:125px;
    height: 25px;
    border:1px solid #d2d2d2;
    outline: none;
}
.topInput>div:nth-child(4)>img{
    position: absolute;
    right: 0;
    top:0;
    bottom: 0;
    height: 100%;
}
.searchB{
    color:#ffffff;
    background: #2196f3;
    width: 68px;
    height: 30px;
    border-radius:2px ;
    text-align: center;
    line-height: 30px;
    font-size: 16px;
    float: right;
    margin-right: 21px;
    cursor: pointer;
}
.tableList{
    margin-left: 32px;
    margin-right: 44px;
    width: 100%;
}
table{
    width: 95%;
    color:#797e80;
    font-size: 14px;
    border: 1px solid #e6e5e4;
    border-top: 1px solid transparent;
    border-bottom:1px solid transparent;
}

table tr td{
    text-align: center;
    border-right: 1px solid #e6e5e4;
    border-bottom:1px solid #e6e5e4;

}
table tr td:last-child{
    border-right:1px solid transparent;
}
table th:last-child{
    border-right:1px solid transparent;
}
table th{
    background: #efefef;
    padding: 8px 7px;
    border-right: 1px solid #e6e5e4;
    border-bottom:1px solid #e6e5e4;
}
table tr td:nth-child(2){
    text-align: left;
    padding: 10px;
}
table tr td:nth-child(3){
    width: 10%;
}
table tr td:nth-child(4){
    text-align: left;
    padding: 10px;
}
table tr td:nth-child(5){
    text-align: left;
    padding: 10px;
}
table tr td:nth-child(6){
    text-align: center!important;
    padding: 15px;
}
table tr td:nth-child(6)>span{
    color:#2196f3;
    margin: 0 13px;
    cursor: pointer;
}
table tr td:nth-child(6)>span img{
    width: 8px;
}

.tableList{
    margin-top: 17px;
}
.chaK{
    color:#2196f3;
    cursor: pointer;
}
.kaoKind,.kaoKind input{
    cursor: pointer;
}
.chaK{
    color:#2196f3;
    cursor: pointer;
}
.kaoKind{
    position: relative;
}
#checkKind{
    position: absolute;
}
.kaoKind,.kaoKind input{
    cursor: pointer;
}
.rulesKind,.rulesKind input{
    cursor: pointer;
}

.rulesKind,.kaoKind,.kaoPKind,.rulesPartyKind{
    position: relative;
}
.selectBtn ul li,.rulerBtn ul li,.PartyMrulerBtn ul li,.selectPartyBtn ul li{
    list-style: none;
    text-align: center;
    width: 100%;
    margin-bottom: 5px;
    padding: 2px 0;
}
.selectBtn,.rulerBtn,.PartyMrulerBtn,.selectPartyBtn{
    z-index: 100000;
    width: 99%;
    position: absolute;
    top: 26px;
    background: #e0e0e0;
    border: 1px solid #d5d5d5;
    height: auto;
    overflow: auto;
    left: 0;
}
.selectBtn ul,.rulerBtn ul,.PartyMrulerBtn ul,.selectPartyBtn ul{
  max-height: 150px;
  overflow: auto;
}
.activeSelcet {
    background: #2198f2 !important;
    color: #ffffff !important;
}
/*弹窗*/
.mask{
    position: fixed;
    top:0;
    left:0;
    bottom: 0;
    right: 0;
    height:100%;
    width: 100%;
    z-index: 1000;
    background: #000000;
    opacity: 0.14;
}
.popBox{
    position: fixed;
    left: 50%;
    margin-left: -275px;
    width: 493px;
    border-radius: 4px;
    z-index: 1001;
    background: #ffffff;
    height: auto;
    top: 5%;
}
.popB_Top{
    border-bottom: 1px solid #c4c4c4;
    padding-bottom: 15px;
    padding-top: 16px;
}
.addW{
    font-size:16px ;
    margin-left: 24px;
    margin-top: 16px;
    margin-bottom: 15px;
}
.close{
    float: right;
    margin-right: 22px;
    width: 20px;
    cursor: pointer;
}
.close img{
    width: 15px;
}
.btnList{
    width: 61%;
    margin: auto;
    overflow: hidden;
    padding-bottom: 27px;
    margin-top: 23px;
}
.btnList input{
    width: 96px;
    height: 32px;
    line-height: 32px;
    text-align: center;
    font-size: 14px;
    padding: 0 0;
    outline: auto;
    border-radius: 4px;
}

.s{
    float: left;
    background: #2198f2;
    color:#ffffff;
    border:1px solid #2198f2;
    cursor: pointer;
}
.c{
    float: right;
    color:#4d4d4d ;
    border: 1px solid #d2d2d2;
    outline: none!important;
    cursor: pointer;
}

.popB_bottom{
    width: 100%;
    font-size: 14px;
    margin-top: 10px;
    padding-bottom: 10px;
    border-bottom: 1px solid #c4c4c4;
    padding-left: 20px;
}
.popB_bottom div{
    display: inline-block;
    margin-right: 10px;
}
.checkList>div input {
    vertical-align: middle;
}
.menuContent{
    width: 48%;
    margin: auto;
    padding-bottom: 31px;
}
.menuContent1{
    width: 48%;
    padding-bottom: 31px;
}

.treeList{
    margin-top: 15px;
    height: 200px;
    overflow: auto;
}
