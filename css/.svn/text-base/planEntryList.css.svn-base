.tableList table{
    color:#797e80;
    font-size: 14px;
    border: 1px solid #e6e5e4;
    border-top: 1px solid transparent;
    border-bottom:1px solid transparent;
}

.tableList  table tr td{
    text-align: center;
    border-right: 1px solid #e6e5e4;
    border-bottom:1px solid #e6e5e4;

}
.tableList table tr td:last-child{
    border-right:1px solid transparent;
}
.tableList table th:last-child{
    border-right:1px solid transparent;
}
.tableList table th{
    padding: 8px 7px;
    border-right: 1px solid #e6e5e4;
    border-bottom:1px solid #e6e5e4;
}
.tableList table tr td:nth-child(2){
    text-align: left;
    padding: 10px;
}
.tableList table tr td:nth-child(3){
    width: 10%;
}
.tableList table tr td:nth-child(4){
    text-align: left;
    padding: 10px;
}
.tableList table tr td:nth-child(5){
    text-align: left;
    padding: 10px;
}
.tableList table tr td:nth-child(6){
    text-align: center!important;
    padding: 15px;
}
.tableList table tr td:nth-child(6)>span{
    color:#2196f3;
    margin: 0 10px;
    cursor: pointer;
}
.tableList table tr td:nth-child(6)>span img{
    width: 8px;
}

