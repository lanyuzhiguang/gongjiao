}
.tableList table {
    margin: auto;
    width: 100%;
    color: #797e80;
    font-size: 14px;
    border: 1px solid #e6e5e4;
    border-top: 1px solid transparent;
    border-bottom: 1px solid transparent;
}
.tableList table th {
    background: #efefef;
    padding: 8px 7px;
    border-right: 1px solid #e6e5e4;
    border-bottom: 1px solid #e6e5e4;
}

.tableList table tr td {
    text-align: center;
    border-right: 1px solid #e6e5e4;
    border-bottom: 1px solid #e6e5e4;
    padding: 10px 10px;
}

.tableList table tr td:nth-child(2){
    text-align: center;
}
.tableList table tr td:nth-child(4){
    text-align: center;
}
.tableList table tr td:nth-child(6)>span{
    text-align: center;
    margin: auto;
    display: inline-block;
    cursor: pointer;
    color:#2196f3;
}
.tableList table tr td:first-child{
    text-align: center;
    padding-left: 10px;
}
.tableList{
    margin-top: 20px;
}
.tableList table tr td:last-child span{
    color:#2196f3;
    cursor: pointer;
    margin-right: 10px;
}

/*弹窗*/
.mask{
    position: absolute;
    top:0;
    left:0;
    bottom: 0;
    right: 0;
    height:100%;
    width: 100%;
    z-index: 1000;
    background: #000000;
    opacity: 0.14;
}
.popBox{
    position: absolute;
    left: 50%;
    margin-left: -275px;
    width: 493px;
    border-radius: 4px;
    z-index: 1001;
    background: #ffffff;
    height: auto;
    top: 10%;
}
.popB_Top{
    border-bottom: 1px solid #c4c4c4;
    padding-bottom: 15px;
    padding-top: 16px;
}
.addW{
    font-size:16px ;
    margin-left: 24px;
    margin-top: 16px;
    margin-bottom: 15px;
}
.close{
    float: right;
    margin-right: 22px;
    width: 20px;
    cursor: pointer;
}
.close img{
    width: 15px;
}
.btnList{
    width: 61%;
    margin: auto;
    overflow: hidden;
    padding-bottom: 27px;
    margin-top: 3px;
}
.btnList input{
    width: 96px;
    height: 32px;
    line-height: 32px;
    text-align: center;
    font-size: 14px;
    padding: 0 0;
    outline: auto;
    border-radius: 4px;
}

.s{
    float: left;
    background: #2198f2;
    color:#ffffff;
    border:1px solid #2198f2;
    cursor: pointer;
}
.c{
    float: right;
    color:#4d4d4d ;
    border: 1px solid #d2d2d2;
    outline: none!important;
    cursor: pointer;
}

.popB_bottom {
    width: 100%;
    font-size: 14px;
    /* margin-top: 10px; */
    padding-bottom: 10px;
    /* border-bottom: 1px solid #c4c4c4; */
    /* padding-left: 20px; */
    margin: auto;
    margin-top: 20px;
}
.popB_bottom>div>div{
    display: inline-block;
    margin-right: 10px;
    margin-bottom: 20px;
}
.popB_bottom>div:nth-child(2)>div>div:nth-child(2){
    position: relative;
}
.popB_bottom>div:nth-child(2)>div:nth-child(2)>img{
    position: absolute;
    top: 0px;
    bottom: 0px;
    right: -1px;
    height: 100%;
    margin-top: 0;
    width: 23px;
    cursor: pointer;
}
.popB_bottom>div>div:first-child{
    width: 30%;
    text-align: right;
}
.popB_bottom>div>div:nth-child(2),.popB_bottom>div>div:nth-child(3){
    width: 101px;
    height: 24px;
    position: relative;
}
.popB_bottom>div>div:nth-child(2)>input{
    width: 100px;
    height: 22px;
    border: 1px solid #c4c4c4;
    border-radius: 2px;
    cursor: pointer;
}
.popB_bottom>div>div:nth-child(3)>input{
    width: 100px;
    height: 22px;
    border: 1px solid #c4c4c4;
    border-radius: 2px;
}
.popB_bottom>div>div:nth-child(2)>img,.popB_bottom>div>div:nth-child(3)>img{
    position: absolute;
    right: 5px;
    top: 50%;
    margin-top: -7px;
    width: 12px;
    cursor: pointer;
}
