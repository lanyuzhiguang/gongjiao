}
.message-navbar{
    background-color: transparent;
}
.page-content{
    margin-top: 20px;
}


ul{
    margin-left: 0;
}
.hotN{
    height: 43px;
    background: #f4f4f4;
    line-height: 43px;
    border-radius: 10px 10px 0 0;
    padding-left: 15px;
    border-bottom: 1px solid #f4f4f4;
    font-size: 16px;
}
.hotNList{
    background: #fafafa;
    width: 100%;
