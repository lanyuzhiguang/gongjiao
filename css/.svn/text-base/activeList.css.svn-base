}
.nav-tabs > li.active > a, .nav-tabs > li.active > a:hover, .nav-tabs > li.active > a:focus{
    background: #FAFAFA;
    border-top: 2px solid red;
}
.nav-tabs{
    border-bottom: 1px solid #dddddd!important;
    /*z-index: 100;*/
}
.nav-tabs > li.active>a{
    color:#333333;
}
