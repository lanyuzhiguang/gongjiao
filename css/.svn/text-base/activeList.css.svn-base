}
.topW,.bottomW {
    width: 100%;
}
.topW label,.bottomW label{
    width: 37%;
}
.activeN{
    height: 30px;
}
.appy,.classify,.time{
    width: 163px;
    height: 30px;
}

.appy,.classify,.time,.activeN{
