}
.radio span{
    font-size: 14px!important;
}
.radio{
    margin-top: 0;
    font-size: 14px;
}
input[type=radio].ace + .lbl::before {
    border-radius: 100%;
    font-size: 1px;
    font-family: FontAwesome;
    line-height: 14px;
    height: 15px;
    min-width: 15px;
    text-shadow: none;
    margin-right: 10px;
}
input[type="radio"].ace + .lbl::before {
    line-height: 14.5px;
    text-shadow: none;
}
.radio label{
    padding-left: 7px;
}
input[type=checkbox].ace + .lbl::before, input[type=radio].ace + .lbl::before{
    color: #CF0000;
    box-shadow: none;
}
#submitB{
    width:80px;
    height: 30px;
    text-align: center;
    line-height: 30px;
    background: #CF0000;
    color:#ffffff;
    border: 1px solid #CF0000;
    padding:0 0;
    border-radius: 10px;
    margin-left: 20%;
    margin-top: 20px;
}
#cancelB{
    width:80px;
    height: 30px;
    text-align: center;
    line-height: 30px;
    background: #d7d7d7;
    color:#ffffff;
    border: 1px solid #d7d7d7;
    padding:0 0;
    border-radius: 10px;
    margin-right: 60%;
    float:right;
    margin-top: 20px;
}
#submitB,#cancelB{
    outline:none;
}
.selectR{
    margin-bottom: 0!important;
}
.edit{
    color:#428bca;
    cursor: pointer!important;
}

.floatC{
    position: absolute;
    left:0;
    top:0;
    bottom:0;
    right:0;
    background: #000000;
    opacity: 0.5;
    z-index: 1000;
}
#editInfo{
    width: 50%;
    height:50%;
    position: absolute;
    left:25%;
    top:8%;
    z-index: 1100;
}

