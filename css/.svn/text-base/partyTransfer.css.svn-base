}
.required{
    color:#CF0000;
}
.transferList{
    margin-top: 20px;
}
.transferList>div{
    margin-bottom:10px;
}
.textB{
    margin-top: 20px;
}
.transferList>div>div{
    display: inline-block;
}
.transferList>div>div:first-child{
    width: 22%;
    display: inline-block;
    text-align: right;
    margin-right: 2%;
    vertical-align: top;
}
.transferList p{
    color: #999999;
