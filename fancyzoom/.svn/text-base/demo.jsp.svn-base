<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
	"http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" lang="en" xml:lang="en">

	<head>

		<meta http-equiv="content-type" content="text/html; charset=UTF-8" />

		<title>jQuery Fancy Zoom plugin</title>		

		<!-- required -->
		<script type="text/javascript" src="jquery.js"></script>
		<!-- optional -->

		<script type="text/javascript" src="jquery.fancyzoom.js"></script>

		<script type="text/javascript">
			$(function(){
				$('img.fancyzoom').fancyzoom({Speed:400,showoverlay:false});
			});
		</script>
		
	   	<style type="text/css">


	   	</style>
	</head>

	<body>
	
		<div id='pageWrapper'>
			<h1>jQuery Fancy Zoom Plugin</h1>



			<h2>Plugin in action</h2>
			<p>
				Click on the image to see the zoomed version.
			</p>
			<p>
				<div id="demo">
				

					<a href='img/2.jpg' id="simplelink"><img class="fancyzoom" src="img/1.jpg" alt="Kopipi island" width="100" /></a>
				</div>
			</p>

		</div>
	</body>

</html>