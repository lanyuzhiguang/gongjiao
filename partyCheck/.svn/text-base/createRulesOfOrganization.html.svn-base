                            "        </div>");
                        isallSelect=3;
                    }
//                  if (itCheckFun()) {
//                  	$("#it").prop("checked", "checked");
//                  }
                    itCheckFun();
                    function itCheckFun(){
                    	for (var ind=0;ind<$("#treeDemo span.chk").length;ind++) {
							if ($("#treeDemo span.chk").eq(ind).hasClass("checkbox_false_disable")) {
								continue;
							} else{
								if ($("#treeDemo span.chk").eq(ind).hasClass("checkbox_true_full") == false) {//有一个未选中 就退出
	                                $("#it").prop("checked", "");
	                                return false;
	                            } else {//选中了 进行下一次循环
	                                continue;
	                            }
							}
						}
                    	$("#it").prop("checked", "checked");
//                  	return true;
                    }
                    var zNodes = [];
                    //tree
                    var setting = {
                        data: {
                            simpleData: {
                                enable: true,
                                idKey: "id",
                                pIdKey: "pId"
                            },
                            key: {
                                name: "name"
                            }
                        },
                        check: {
                            enable: true,
                            chkStyle: "checkbox",
                            chkboxType: {"Y": "", "N": ""}
                        },
                        callback: {
                            onCheck: onCheck
                        }
                    };
