                                //报上级党委审批
                                $($("#waitHandler ul li")[8]).click(function () {
                                    // if(allData.ruObject.f2i != "Y"){
                                    if (allData.ruObject.f2a == "Y" && allData.ruObject.f2b == "Y" && allData.ruObject.f2c == "Y" &&  allData.ruObject.f2d == "Y" && allData.ruObject.f2e == "Y" && allData.ruObject.f2f == "Y" && allData.ruObject.f2g == "Y"&& allData.ruObject.f2h == "Y") {
                                        initPoper("报上级党委审批", "该同志资料齐全，报上级党委审批",function () {
                                            $.ajax({
                                                type: "POST",
                                                url: path + "/recruit/upAdut.action",
                                                data: {
                                                    "recid": recId
                                                },
                                                dataType: "json",
                                                success: function (data) {
                                                    if(data.success==true){
                                                        window.location.reload();
                                                    }else{
                                                        parent.layer.alert(data.message);
                                                    }
                                                }
                                            })
                                        });
                                    } else {
                                        parent.layer.alert("该同志资料不全，请确认资料齐备后再提交");

                                    }
                                    // }

                                });


                            }

                        
                        });
                        if(allData.type!=3){
                            if(allData.isPerpl==2 || allData.isPerpl==5 ){
                                $("#shState").addClass("hid");
                                $("#accessBtn").removeClass("hid");
                            }
                            if(allData.isPerpl==1||allData.isPerpl==2){
                                $("#state").text("党委预审");
                                $("#shState").click(function () {
                                    initPoper("预审", "是否同意通过审核",function () {
                                        $.ajax({
                                            type: "POST",
                                            url: path + "/recruit/auditing.action",
                                            data: {
                                                "recid": recId,
                                                "type": 2,
                                                "state": 2
                                            },
                                            dataType: "json",
                                            success: function (data) {
                                                if (data.success == true) {
                                                    window.location.reload();
                                                } else {
                                                    parent.layer.alert(data.message);
                                                }
                                            }
                                        })
                                    })
                                })
                            }
                            if(allData.isPerpl==3){
                                $("#state").text("党委审核");
                                $("#shState").click(function () {
                                    initPoper("审核", "是否同意通过审核，并提交上一级党委备案",function () {
                                        $.ajax({
                                            type: "POST",
                                            url: path + "/recruit/auditing.action",
                                            data: {
                                                "recid": recId,
                                                "type": 2,
                                                "state": 4
                                            },
                                            dataType: "json",
                                            success: function (data) {
                                                if (data.success == true) {
                                                    window.location.reload();

                                                } else {
                                                    parent.layer.alert(data.message);
                                                }
                                            }
                                        })
                                    })
                                })
                            }
                            if(allData.isPerpl==5){
                                $("#state").text("党委审核备案");
                                $("#shState").click(function () {
                                    initPoper("审核备案", "同意该同志列为预备党员",function () {
                                        $.ajax({
                                            type: "POST",
                                            url: path + "/recruit/auditing.action",
                                            data: {
                                                "recid": recId,
                                                "type": 2,
                                                "state": 5
                                            },
                                            dataType: "json",
                                            success: function (data) {
                                                if (data.success == true) {
                                                   window.location.reload();
                                                } else {
                                                    parent.layer.alert(data.message);
                                                }
                                            }
                                        })
                                    })
                                })
                            }
                            if(allData.isPerpl==4){
                                $.ajax({
                                    type: "POST",
                                    url: path + "/recruit/bumenToCode.action",
