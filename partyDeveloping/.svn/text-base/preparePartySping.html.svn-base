                                    success: function (data) {
                                        if (data.success == true) {//根据部门判断是否拥有审批权限
                                            $("#state").text("党委审核待备案");
                                            $("#shState").click(function () {
                                                initPoper("审核备案", "同意该同志列为预备党员",function () {
