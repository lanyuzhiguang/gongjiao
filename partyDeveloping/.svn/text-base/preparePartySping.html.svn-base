                                                            "recid": recId,
                                                            "type": 2,
                                                            "state": 5
                                                        },
                                                        dataType: "json",
                                                        success: function (data) {
                                                            if (data.success == true) {
                                                                window.location.reload();
                                                            } else {
                                                                parent.layer.alert(data.message);
                                                            }
                                                        }
                                                    })
                                                })
                                            })

                                        } else {
                                        	$("#shState").addClass("hid");
                                        	$("#state").text("审批通过，待备案");
