                                        parent.layer.alert('请选择日期！');
                                        return;
                                    }
                                    $.ajax({
                                        type: "POST",
                                        url: path + "/recruit/volbook.action",
                                        data: {"recid": recId, "volbookData": $("#date").val()},
                                        dataType: "json",
                                        success: function (data) {
                                            window.location.reload();
                                        }
                                    })
                                });
                                //支部大会决议
                                $($("#waitHandler ul li")[6]).click(function () {
